		"@tiptap/starter-kit": "^2.22.0",
		"lucide-svelte": "^0.513.0",
		"patch-package": "^8.0.0",
		"svelte-hero-icons": "^5.2.0",
		"svelte-pdf": "^1.0.28",
		"tailwindcss-animate": "^1.0.7"
	}
